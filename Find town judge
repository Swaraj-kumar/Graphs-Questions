class Solution {
public:
    int findJudge(int n, vector<vector<int>>& trust) {
        vector<int>temp(n+1,0);
        for(auto it: trust){
            temp[it[0]]--;//outdegree
            temp[it[1]]++;//indegree
        }
        
        for(int i=0;i<=n;i++){
            if(temp[i] == n-1)return i;
        }
        return -1;
    }
};
